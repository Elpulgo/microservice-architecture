            "UpstreamPathTemplate": "/api/batch/{everything}",
            "UpstreamHttpMethod": [
                "POST",
                "OPTIONS",
                "FETCH",
                "GET"
            ]
        },
        {
            "DownstreamPathTemplate": "/api/roundtrip",
            "DownstreamScheme": "http",
            "ServiceName": "roundtrip-service",
            "UpstreamPathTemplate": "/api/roundtrip",
            "UpstreamHttpMethod": [
                "POST",
                "OPTIONS",
                "FETCH",
                "GET"
            ]
        },
        {
            "DownstreamPathTemplate": "/ws",
            "DownstreamScheme": "ws",
